{
    "name": "jotaelesalinas/php-github-sponsors",
    "type": "library",
    "description": "See your GitHub sponsors",
    "keywords": [
        "jotaelesalinas",
        "php-github-sponsors"
    ],
    "homepage": "https://github.com/jotaelesalinas/php-github-sponsors",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Luis Salinas",
            "email": "jlsalinas@example.com",
            "homepage": "https://github.com/jotaelesalinas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/collections": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "psr/simple-cache": "^1.0",
        "illuminate/cache": "^7.0|^8.0",
        "spatie/data-transfer-object": "^2.5"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "guzzlehttp/guzzle": "^7.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^7.5|^8.5|^9.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JLSalinas\\GithubSponsors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JLSalinas\\GithubSponsors\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
